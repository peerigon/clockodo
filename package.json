{
  "name": "clockodo",
  "version": "0.0.0-semantically-released",
  "description": "Unofficial JavaScript/TypeScript SDK for Clockodo",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "node": "./dist/cjs/index.mjs",
      "default": "./dist/esm/index.js"
    },
    "./csv": {
      "require": "./dist/cjs/csv.js",
      "node": "./dist/cjs/csv.mjs",
      "default": "./dist/esm/csv.js"
    },
    "./mocks": {
      "require": "./dist/cjs/mocks.js",
      "node": "./dist/cjs/mocks.mjs",
      "default": "./dist/esm/mocks.js"
    }
  },
  "typesVersions": {
    "*": {
      "csv": [
        "./dist/esm/csv.d.ts"
      ],
      "mocks": [
        "./dist/esm/mocks.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:clean": "rimraf dist",
    "build:tsc": "run-p build:tsc:*",
    "build:tsc:cjs": "tsc -p ./tsconfig.cjs.json",
    "build:tsc:esm": "tsc -p ./tsconfig.esm.json",
    "build:copy": "copyfiles -f ./src/*.mjs ./dist/cjs/",
    "test": "run-p test:*",
    "test:suite": "jest",
    "test:suite:watch": "jest --watch",
    "test:turkish": "cross-env LC_ALL=tr jest 'integration' -t 'using a Turkish locale'",
    "test:lint": "eslint --max-warnings 0 --cache --ext js,ts --ignore-path .gitignore src",
    "posttest": "run-s build",
    "prepare": "husky install",
    "release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peerigon/clockodo.git"
  },
  "keywords": [
    "time tracking",
    "api",
    "sdk"
  ],
  "author": "developers@peerigon.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/peerigon/clockodo/issues"
  },
  "homepage": "https://github.com/peerigon/clockodo#readme",
  "dependencies": {
    "@faker-js/faker": "^6.1.1",
    "map-obj": "^4.3.0",
    "node-fetch": "^2.6.7",
    "qs": "^6.10.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "@types/node-fetch": "^2.6.1",
    "@types/qs": "^6.9.7",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint": "^8.2.0",
    "eslint-config-peerigon": "^33.0.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "jest-date-mock": "^1.0.8",
    "lint-staged": "^11.2.6",
    "nock": "^13.2.1",
    "npm-run-all": "^4.1.5",
    "pin-github-action": "^1.4.0",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^18.0.0",
    "ts-jest": "^27.0.7",
    "ts-jest-resolver": "^2.0.0",
    "typescript": "^4.4.4"
  },
  "files": [
    "dist",
    "plugins"
  ],
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "eslint --cache --fix"
    ],
    ".github/workflows/*.{yml,yaml}": [
      "pin-github-action --allow-empty"
    ],
    "*.{json,yml,md}": "prettier --write"
  }
}
