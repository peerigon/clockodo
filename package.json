{
  "name": "clockodo",
  "version": "0.0.0-semantically-released",
  "description": "Unofficial JavaScript/TypeScript SDK for Clockodo",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "build": "run-s build:*",
    "build:clean": "rimraf dist",
    "build:tsc": "tsc -p tsconfig.build.json",
    "lint": "eslint --max-warnings 0 --cache --no-ignore",
    "test": "run-p test:*",
    "test:unit": "vitest run",
    "test:lint": "eslint --max-warnings 0 --cache --ext js,ts --ignore-path .gitignore .",
    "test:format": "prettier --check --ignore-path .gitignore .",
    "test:types": "tsc --noEmit",
    "prepublishOnly": "run-s build",
    "release": "semantic-release"
  },
  "exports": {
    ".": {
      "types": "./dist/exports/main.exports.d.ts",
      "default": "./dist/exports/main.exports.js"
    },
    "./csv": {
      "types": "./dist/exports/csv.exports.d.ts",
      "default": "./dist/exports/csv.exports.js"
    },
    "./mocks": {
      "types": "./dist/exports/mocks.exports.d.ts",
      "default": "./dist/exports/mocks.exports.js"
    }
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peerigon/clockodo.git"
  },
  "keywords": [
    "time tracking",
    "api",
    "sdk"
  ],
  "author": "developers@peerigon.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/peerigon/clockodo/issues"
  },
  "homepage": "https://github.com/peerigon/clockodo#readme",
  "dependencies": {
    "@faker-js/faker": "^9.4.0",
    "axios": "^1.0.0",
    "map-obj": "^5.0.1",
    "p-limit": "^6.2.0",
    "qs": "^6.10.5"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/qs": "^6.9.18",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "eslint": "^8.57.1",
    "eslint-config-peerigon": "^39.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "nock": "^14.0.0",
    "npm-run-all2": "^7.0.2",
    "pin-github-action": "^2.1.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.2.1",
    "timezone-mock": "^1.3.6",
    "typescript": "^5.7.3",
    "vitest": "^3.0.4"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "eslint --cache --fix"
    ],
    ".github/workflows/*.{yml,yaml}": [
      "pin-github-action --allow-empty"
    ],
    "*.{json,yml,md}": "prettier --write"
  }
}
