{
  "name": "clockodo",
  "version": "4.0.0",
  "description": "Unofficial Node.js SDK for the Clockodo API",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc -p src/tsconfig.json",
    "dev": "npm run build -- -w",
    "lint": "eslint src tests --ext js,ts",
    "test": "jest",
    "test:unit": "jest tests/unit/unit.test.ts",
    "test:types": "tsc --noEmit",
    "posttest": "run-p lint test:types",
    "prepublishOnly": "npm run build",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peerigon/clockodo.git"
  },
  "keywords": [
    "time tracking",
    "api",
    "sdk"
  ],
  "author": "developers@peerigon.com",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/peerigon/clockodo/issues"
  },
  "homepage": "https://github.com/peerigon/clockodo#readme",
  "dependencies": {
    "axios": "0.19.0",
    "axios-cache-adapter": "^2.5.0",
    "camelcase-keys": "^6.2.2",
    "moment": "^2.24.0",
    "qs": "^6.7.0"
  },
  "devDependencies": {
    "@types/jest": "^24.9.0",
    "@types/node": "^12.6.8",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "dotenv": "^8.0.0",
    "eslint": "^6.1.0",
    "eslint-config-peerigon": "^25.1.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-prefer-arrow": "^1.1.7",
    "jest": "^24.8.0",
    "nock": "^10.0.6",
    "npm-run-all": "^4.1.5",
    "standard-version": "^6.0.1",
    "ts-jest": "^24.3.0",
    "typescript": "^3.5.3"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "collectCoverage": true,
    "testEnvironment": "node"
  }
}
